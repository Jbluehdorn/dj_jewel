    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "bootstrap": "^4.5.2",
    "electron": "^10.1.3",
    "laravel-mix": "^5.0.4",
    "node-sass": "^4.14.1",
